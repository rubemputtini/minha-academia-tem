{
  "ConnectionStrings": {
    "DefaultConnection": "${{ConnectionStrings__DefaultConnection}}"
  },
  "StorageConnectionString": "${{StorageConnectionString}}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AdminSettings": {
    "AdminEmail": "${{AdminSettings__AdminEmail}}"
  },
  "JwtKey": "${{JwtKey}}",
  "Smtp": {
    "Host": "${{Smtp__Host}}",
    "Port": "${{Smtp__Port}}",
    "UserName": "${{Smtp__UserName}}",
    "Password": "${{Smtp__Password}}"
  },
  "AllowedHosts": "*",
  "AllowedOrigins": [
    "http://localhost:3000",
    "https://academia.rubemputtini.com.br/"
  ]
}
